define FATE_HEVC_QSV_TEST
FATE_HEVC_QSV += fate-hevc-qsv-conformance-$(1)
fate-hevc-qsv-conformance-$(1): CMD = framecrc -flags unaligned -vsync drop -r 25 -c:v hevc_qsv -i $(TARGET_SAMPLES)/hevc-conformance/$(1).bit -pix_fmt yuv420p
fate-hevc-qsv-conformance-$(1): REF = $(SRC_PATH)/tests/ref/fate/hevc-conformance-$(1)
endef

